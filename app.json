{
  "expo": {
    "name": "adopt-a-friend",
    "slug": "adopt-a-friend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo-register.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/logo-register.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo-register.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/img/logo-register.png"
    },
    "plugins": ["expo-image-picker"]

  }
}
